{
    "manifest_version": 3,
    "name": "Screenshot Tagger",
    "version": "1.0",
    "description": "Capture screenshots, add tags, and insert into Google Docs or Sheets.",
    "permissions": [
    "notifications",
    "contextMenus",
    "identity",
    "storage",
    "https://www.googleapis.com/",
    "activeTab"
]
,
    "oauth2": {
  "client_id": "535986854348-j8vhqkafcci1g4b43c1o91l6n5rk1s91.apps.googleusercontent.com",
  "scopes": [
    "https://www.googleapis.com/auth/documents",
    "https://www.googleapis.com/auth/spreadsheets",
    "https://www.googleapis.com/auth/drive.file",
    "https://www.googleapis.com/auth/drive"
  ]
},
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "commands": {
      "take_screenshot": {
        "suggested_key": {
          "default": "Ctrl+Shift+S"
        },
        "description": "Take a screenshot and tag it"
      }
    }
  }
  